alias uncomment='sed "s/\s*#.*//g; /^$/ d"'
filealias0(){ p=$@;for L in $(uncomment ~/.shrc/aliases/"$p");do alias $(echo $L |cut -d'=' -f 1 )="$p $(echo $L |cut -d'=' -f 2- )";done;}
filealias1(){ a=$@;for L in $(uncomment ~/.shrc/aliases/"$a");do alias $L="$L $a";done;}

filealias0 w3m
filealias0 flatpak run
filealias1 --color=auto
for L in $(uncomment ~/.shrc/aliases/error);do alias $L="$L 2> /dev/null";done

